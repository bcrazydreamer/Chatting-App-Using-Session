<!DOCTYPE html>
<html>
<head>
	<title>Profile</title>

  <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel='stylesheet' href='/stylesheets/style.css' />

    <link rel="stylesheet" type="text/css" href="http://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="../Plugins/css/notie.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.18/css/dataTables.bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/plug-ins/1.10.18/integration/font-awesome/dataTables.fontAwesome.css">

    <link rel="stylesheet" href="../stylesheets/CustomDatatable.css">
		<link rel="stylesheet" href="../stylesheets/datatable.css">

    <link rel="stylesheet" href="../Plugins/css/trumbowyg.css">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.0/jquery-confirm.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.0/jquery-confirm.min.js"></script>
  <link rel="stylesheet" href="https://formden.com/static/cdn/bootstrap-iso.css" />
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>
<body>
<div style="position: fixed;width: 100%;height: 1px"></div>

<header class="TopHeader">
	<img src="../Upload/Profile/<%= UserData.profilePic %>" class="profile-pic profilePic" id='top-header-profile-pic'>
</header>
	<div class="header-content allSides" id="fixHeader">
		<img src="../Upload/Profile/<%= UserData.profilePic %>" class="profile-pic-fix-head" id='fixed-header-profile-pic'>
		<div class="align-center-all">
					<!-------------------------------------------------------------------->
					<div class="col-xs-4 mobile-fixed-header-profile mobile-head-user-info">
					</div>
					<div class="col-xs-7 mobile-fixed-header-profile">
					  	<div class="col-xs-8 mobile-head-user-info">
					  		<center>
					  			<p class="header-username"><%= UserData.fullname %></p>
					  			<p class="header-status"><%= UserData.role %></p>
					    	</center>
					 	</div>
					 	<div class="col-xs-4 mobile-head-user-info">
					 		<div class="dropdown">
					 			<button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown"><i class="fa fa-bars"></i></button>
							    <ul class="dropdown-menu dropdown-menu-right" style="margin-top: 60%">
							      <li><a href="/admin/profile">Home</a></li>

							      <li class="divider"></li>
							<%if (UserData.role=='admin') { %>
							      <li><a href="/admin/adduser">Add User</a></li>
							      <li><a href="/admin/userlist">User List</a></li>
							<% } %>
							      <li><a href="/community/AddCommunity">Create Community</a></li>
							      <li><a href="/community/communityList">Community List</a></li>


							      <li><a href="#">Notifications <span class="badge">10</span></a></li>
							      <li class="divider"></li>
							      <li><a onclick="logout()">Logout</a></li>
							    </ul>
							</div>
					 	</div>
					</div>
					<!-------------------------------------------------------------------->

				<div class="row">
					<div class="col-sm-1 center-header-keys-pc">
					</div>

			 			<div class="col-sm-8 center-header-keys-pc">
							  <div class="col-sm-3">
							  	<center>
							  		<p class="header-username"><%= UserData.fullname %></p>
							  		<p class="header-status"><%= UserData.role %></p>
							    </center>
							  </div>

							  <div class="col-sm-9">
							  	<div class="col-sm-2">
							  		<a class="btn btn-default allSideButton header-buttons" href="/profile">Home</a>
							  	</div>
					<%if (UserData.role!='admin') { %>
								<div class="col-sm-2">
							  		<a class="btn btn-default allSideButton header-buttons" href="/editProfile">Profile</a>
							  	</div>

							  	<div class="col-sm-2">
							  		<!-- <a class="btn btn-default allSideButton header-buttons">
							  			Community
							  		</a> -->
							  	</div>
							  	<div class="col-sm-4">
							  		<div class="dropdown">
								 			<button class="btn btn-default dropdown-toggle header-buttons allSideButton" type="button" data-toggle="dropdown">
								 				Community Manager
								 			</button>
										    <ul class="dropdown-menu dropdown-menu-right" style="margin-top:12%">
										      <li>
										      	<a href="/community/AddCommunity">
										      	Create Community
										      	</a>
										      </li>
										      <li>
										      	<a href="/community/communityList">
										      	Community List
										      	</a>
										      </li>
										    </ul>
										</div>
							  	</div>
					<% } %>
					<%if (UserData.role=='admin') { %>
							  	<div class="col-sm-2">
							  		<div class="dropdown">
							 			<button class="btn btn-default dropdown-toggle header-buttons allSideButton" type="button" data-toggle="dropdown">
							 				Admin
							 			</button>
									    <ul class="dropdown-menu dropdown-menu-right" style="margin-top:40%">

									      <li><a href="/admin/adduser">Add User</a></li>
									      <li><a href="/admin/userlist">User List</a></li>
									      <li><a href="/community/AddCommunity">Create Community</a></li>
									      <li><a href="/community/communityList">Community List</a></li>

									    </ul>
									</div>
							  	</div>
							  	<div class="col-sm-6">
							  	</div>
					<% } %>
							  	

							  	<div class="col-sm-2">
							  		<div class="dropdown">
							 			<button class="btn btn-default dropdown-toggle header-buttons allSideButton" type="button" data-toggle="dropdown">
							 				<i class="fa fa-cog font-icon-head"></i>
							 			</button>
									    <ul class="dropdown-menu dropdown-menu-right" style="margin-top:40%">
									      <li><a href="#">Notifications <span class="badge">10</span></a></li>
									      <li><a href="/admin/changePassword">Change Password</a></li>
									      <li class="divider"></li>
									      <li><a onclick="logout()">Logout</a></li>
									    </ul>
									</div>
							  	</div>

							  </div>
			 			</div>

		  				<div class="col-sm-3 center-header-keys-pc">
							 <div class="col-sm-12">
							  		<div style="display: flex;">
							  			<input type="text" name="" class="form-control search-bar" style="border-top-right-radius:0;border-bottom-right-radius:0;" placeholder="Search">
							  			<a class="btn btn-default search-bar-submit" style="border-top-left-radius:0;border-bottom-left-radius:0;"><i class="fa fa-search font-icon-head"></i></a>
									</div>
							</div>
		  				</div>
		  		</div>
		</div>
	</div>


<div class="content">
